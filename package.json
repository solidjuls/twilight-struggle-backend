    "@types/node": "^24.7.2",
    "@types/passport-jwt": "^4.0.1",
    "nodemon": "^3.1.10",
    "prisma": "6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
